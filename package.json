    "elm": "^0.18.0",
    "elm-webpack-loader": "^4.3.0",
    "eslint": "^4.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.1",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  }
